# uguu window
ctrl + shift + 2
	uguush -w

# uguu fullscreen
ctrl + shift + 3
	uguush -f

# uguu selection
ctrl + shift + @4
	uguush -s

# dmenu
super + d
	dmenu_run -fn "-*-orp-medium-r-*--*-*-*-*-*-*-*-*" -p "   execute   " -y 39 -x 7 -h 20 -w 190 -l 15 -nb '#1F1F1F' -nf '#EBEBEB' -sb '#1F1F1F' -sf '#6C887A'
